{
    "name": "bethinkpl/elastic-scout",
    "description": "Elasticsearch Driver for Laravel Scout",
    "keywords": ["elasticsearch", "laravel", "php", "laravel-scout"],
    "require": {
        "php": ">=7.0,
        "laravel/scout": "^8.0",
        "elasticsearch/elasticsearch": "^7.12.0"
    },
     "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ScoutEngines\\Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoutEngines\\Elasticsearch\\Test\\": "tests"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^5.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutEngines\\Elasticsearch\\ElasticsearchProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
